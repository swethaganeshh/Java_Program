import java.io.*;
import java.util.*;
class HelloWorld {
    static void reverse(String s){
        int len=s.length();
        for(int i=len-1;i>=0;i--){
            System.out.print(s.charAt(i));
        }
         System.out.print(" ");
    }
    static void reve(String s){
        String x[]=s.split(" ");
        int len=x.length;
        for(int i=0;i<len;i++){
            reverse(x[i]);
        }
    }
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String str=s.nextLine();
        reve(str);
    }
}
